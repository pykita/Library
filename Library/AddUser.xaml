<Window x:Class="Library.AddUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddUser" Height="328" Width="378">
    <Grid>
        <Button x:Name="addUser_Click" Content="Add" HorizontalAlignment="Left" Margin="53,232,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="cancel_click" Content="Cancel" HorizontalAlignment="Left" Margin="202,232,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="UserName" HorizontalAlignment="Left" Height="23" Margin="157,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="UserSecondName" HorizontalAlignment="Left" Height="23" Margin="157,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="UserPhoneNumber" HorizontalAlignment="Left" Height="23" Margin="157,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="UserAdress" HorizontalAlignment="Left" Height="23" Margin="157,182,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="53,56,0,0" VerticalAlignment="Top"/>
        <Label Content="Second Name" HorizontalAlignment="Left" Margin="53,98,0,0" VerticalAlignment="Top"/>
        <Label Content="Phone Number" HorizontalAlignment="Left" Margin="53,142,0,0" VerticalAlignment="Top"/>
        <Label Content="Adress" HorizontalAlignment="Left" Margin="53,182,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
